{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "frontend/.next",
  "devCommand": "npm run dev",
  "rewrites": [
    { "source": "/(.*)", "destination": "/frontend/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "github": {
    "silent": true
  },
  "crons": [],
  "functions": {},
  "version": 2,
  "projects": [
    {
      "root": "frontend"
    }
  ]
} 